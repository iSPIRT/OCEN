{
	"$schema": "http://json-schema.org/draft-07/schema#", 
	"$id": "https://github.com/iSPIRT/lsp-lender-protocol-specification/blob/master/Core/schema/Version%200.0.1/rejectiondetail.json",  
	"type": "object",
	"required": [
		"reason",
		"description"
	],
	"properties": {
		"reason": { 
			"type": "string",
			"enum": [
				"LOW_CREDIT_SCORE","FRAUD","DOC_IRREGULARITIES","OTHERS"
			]
		},
		"description": { 
			"type": "string",
			"examples": [
				"Credit score below 600"
			]
		},
		"url":{
			"type":"string"
		},
		"extensibleData":{
			"$ref": "https://github.com/iSPIRT/lsp-lender-protocol-specification/blob/master/Core/schema/Version%200.0.1/extensibleData.json"
		}
	}
}

 