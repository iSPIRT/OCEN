{
	"$schema": "http://json-schema.org/draft-07/schema#", 
	"$id": "https://github.com/iSPIRT/lsp-lender-protocol-specification/blob/master/Meta%20APIs/schema/Verion%200.0.1/Get%20Error%20Code/GetErrorCodeResponse.json",  
	"type": "object",
	"required": [
		"metadata",
		"response",
		"requestId",
		"errorCodes"
	],
	"properties": {
		"metadata": {
			"$ref": "https://github.com/iSPIRT/lsp-lender-protocol-specification/blob/master/Core/schema/Version%200.0.1/metadata.json"
		}
,
		"response": { 
			"$ref": "https://github.com/iSPIRT/lsp-lender-protocol-specification/blob/master/Core/schema/Version%200.0.1/response.json"
		}
,
		"requestId": { 
			"type": "string"
		},
		"errorCodes": { 
			"type": "array",
			"items":{
				"type": "object",
				"required": [
					"code",
					"description"
				],
				"properties": {
					"code": {
						"type": "string",
						"examples": [
							"SDP1001"
						]
					},
					"description": {
						"type": "string",
						"examples": [
							"Selected disbursement plan no longer valid"
						]
					}
				}
			}

		}
	}
}
