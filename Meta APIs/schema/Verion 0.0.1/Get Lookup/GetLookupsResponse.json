{
	"$schema": "http://json-schema.org/draft-07/schema#", 
	"$id": "https://github.com/iSPIRT/lsp-lender-protocol-specification/blob/master/Meta%20APIs/schema/Verion%200.0.1/Get%20Lookup/GetLookupsResponse.json", 
	"type": "object",
	"required": [
		"metadata",
		"response",
		"requestId",
		"lookupType",
		"lookupCodes"
	],
	"properties": {
		"metadata": {
			"$ref": "https://github.com/iSPIRT/lsp-lender-protocol-specification/blob/master/Core/schema/Version%200.0.1/metadata.json"
		}
,
		"response": {
			"$ref": "https://github.com/iSPIRT/lsp-lender-protocol-specification/blob/master/Core/schema/Version%200.0.1/response.json"
		}
,
		"requestId": {
			"type": "string"
		},
		"lookupType": { 
			"type": "string",
			"examples": [
				"LOAN_STATUS"
			]
		},
		"lookupCodes": { 
			"type": "array",
			"items":{
				"type": "string",
				"examples": [
					"PROCESSING","OFFERED","GRANTED","REJECTED","ACTION REQUIRED"
				]
			}
		}
	}
}
